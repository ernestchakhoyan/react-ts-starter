{
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false,
    "importOrder": ["^react", "^(?!(react|@core/))\\w+", "^@material", "", "^@(core|ui)/", "^."],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": false,
    "importOrderCaseInsensitive": true
}
